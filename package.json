    "eslint": "5.14.1",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-jasmine": "2.10.1",
    "eslint-plugin-jest": "22.3.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "ima-examples": "^0.16.0",
    "ima-gulp-task-loader": "^0.16.0",
